                    data-aos="fade-up" required />
                  <input type="text" placeholder="subject" required
                    name="subject" data-aos="fade-up" />
                </div>

                <textarea placeholder="your message" name="message" id=""
                  cols="30" rows="10" required
                  data-aos="fade-up"></textarea>

